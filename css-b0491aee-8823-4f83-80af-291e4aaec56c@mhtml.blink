@charset "utf-8";

* { padding: 0px; margin: 0px; }

html.fixedDocument { position: fixed; inset: 0px; width: 100%; }

.loaderWrapper { height: 100vh; width: 100vw; background-color: rgb(0, 0, 0); position: absolute; z-index: 1000; top: 0px; left: 0px; }

.loaderImg { position: absolute; top: 50%; left: 50%; width: 10%; max-width: 60px; transform: translate(-50%, -50%); }
